{
    "name": "sunshinev/laravel-gii",
    "authors": [
        {
            "name": "sunshinev"
        }
    ],
    "keywords": ["laravel", "gii", "code generator", "GUI","CRUD"],
    "license":"MIT",
    "description": "visual code generator tools gii for laravel",
    "require": {
        "sebastian/diff": "^3.0",
        "sunshinev/laravel-fe-render":"^1.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.8|^4.0"
    },
    "autoload": {
        "psr-4":{
            "Sunshinev\\Gii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunshinev\\Gii\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sunshinev\\Gii\\Providers\\GiiServiceProvider"
            ]
        }
    }
}
